{
    "developer.rackspace.com": [
        {
            "from": "^\\/docs\\/user-guides\\/?$",
            "to": "/docs/user-guides/infrastructure/",
            "rewrite": false,
            "status": 301
        },
        {
            "description": "Force trailing slash",
            "from": "\\/([a-zA-Z0-9%_-]*?[^/])$",
            "to": "/$1/",
            "rewrite": false,
            "status": 301
        },
        {
            "description": "Blog: Remove .html extension",
            "from": "^\\/blog\\/(.+?)(\\.html)$",
            "to": "/blog/$1/",
            "rewrite": false,
            "status": 301
        },
        {
            "from": "^\\/devtrial",
            "to": "/signup/",
            "rewrite": false,
            "status": 301
        },
        {
            "from": "^\\/developer-plus\\/?$",
            "to": "/signup/",
            "rewrite": false,
            "status": 301
        },
        {
            "from": "^\\/signup",
            "to": "/cloud",
            "toProtocol": "https",
            "toHostname": "cart.rackspace.com",
            "rewrite": false,
            "status": 301
        },
        {
            "from": "^\\/sdks\\/?$",
            "to": "/docs/#sdks",
            "rewrite": false,
            "status": 301
        },
        {
            "from": "^\\/docs\\/cloud-autoscale\\/v1\\/developer-guide\\/?(.*)$",
            "to": "/docs/autoscale/v1/developer-guide/$1",
            "rewrite": false,
            "status": 301
        },
        {
            "description": "Redirect cdn developer-guide URL",
            "from": "^\\/docs\\/cloud-cdn\\/v1\\/developer-guide\\/?(.*)$",
            "to": "/docs/cdn/v1/$1",
            "rewrite": false,
            "status": 301
        },
        {
          "description": "Redirect cloud dns developer-guide URL",
          "from": "^\\/docs\\/cloud-dns\\/v2\\/developer-guide\\/(.*?)",
          "to": "/docs/cloud-dns/v2/$1",
          "rewrite": false,
          "status": 301
        },
        {
            "description": "Redirect load balancers developer-guide URL",
            "from": "^\\/docs\\/cloud-load-balancers\\/v1\\/developer-guide\\/(.*?)",
            "to": "/docs/cloud-load-balancers/v1/$1",
            "rewrite": false,
            "status": 301
        },
        {
            "description": "Redirect servers developer-guide URL",
            "from": "^\\/docs\\/cloud-servers\\/v2\\/developer-guide\\/(.*?)",
            "to": "/docs/cloud-servers/v2/$1",
            "rewrite": false,
            "status": 301
        },
        {
            "from": "^\\/docs\\/cloud-metrics\\/v1\\/developer-guide\\/?(.*)$",
            "to": "/docs/metrics/v1/developer-guide/$1",
            "rewrite": false,
            "status": 301
        },
        {
            "from": "^\\/docs\\/metrics\\/v1\\/developer-guide\\/?(.*)$",
            "to": "/docs/metrics/v2/developer-guide/$1",
            "rewrite": false,
            "status": 301
        },
        {
            "from": "^\\/docs\\/cloud-monitoring\\/v1\\/developer-guide\\/?(.*)$",
            "to": "/docs/rackspace-monitoring/v1/developer-guide/$1",
            "rewrite": false,
            "status": 301
        },
        {
            "from": "^\\/docs\\/cloud-networks\\/v1\\/developer-guide\\/?(.*)$",
            "to": "/docs/cloud-networks/v2/developer-guide/$1",
            "rewrite": false,
            "status": 301
        },
        {
            "description": "Redirect orchestration developer-guide URL",
            "from": "^\\/docs\\/cloud-orchestration\\/v1\\/developer-guide\\/(.*?)",
            "to": "/docs/cloud-orchestration/v1/$1",
            "rewrite": false,
            "status": 301
        },
        {
            "from": "^\\/logs\\/?$",
            "to": "/",
            "toProtocol": "https",
            "toHostname": "iad1-10096-kibana.es.objectrocket.com",
            "toPort": "20096",
            "rewrite": false,
            "status": 302
        },
        {
          "description": "Redirect vCenter doc to URL for Managed VMWare services",
          "from": "^\\/docs/private-cloud\\/dedicated-vcloud\\/vcenter-handbook\\/?(.*)$",
          "to": "/docs/managed-vmware-services/vcenter/$1",
          "rewrite": false,
          "status": 301
        },
        {
          "description": "Redirect vCloud v1.5 doc to URL for Managed VMWare services",
          "from": "^\\/docs\\/private-cloud\\/dedicated-vcloud\\/vcloud-handbook-v1.5\\/?(.*)$",
          "to": "/docs/managed-vmware-services/vcloud/v1.5/?(.*)$",
          "rewrite": false,
          "status": 301
        },
        {
          "description": "Redirect vCloud v1.0 doc to URL for managed VMWare services",
          "from": "^\\/docs\\/private-cloud\\/dedicated-vcloud\\/vcloud-handbook\\/?(.*)$",
          "to": "/docs/managed-vmware-services/vcloud/v1//$1",
          "rewrite": false,
          "status": 301
        },
        {
          "description": "Redirect dedicated vCloud doc menu to managed VMWare services menu",
          "from": "^\\/docs\\/private-cloud\\/dedicated-vcloud\\/?(.*)$",
          "to": "/docs/#docs-managed-vmware-services",
          "rewrite": false,
          "status": 301
        },
        {
          "description": "Redirect RPC URL to current version",
          "from": "^/docs/private-cloud/rpc/?$",
          "to": "/docs/private-cloud/rpc/v12",
          "rewrite:": false,
          "status": 301
        },
        {
          "description": "Redirect getting started URLs to quickstart content",
          "from": "^\\/docs\\/([^/]+?)\\/getting-started\\/(.*?)",
          "to": "/docs/$1/quickstart/$2",
          "rewrite": false,
          "status": 301
        }
    ]
}
